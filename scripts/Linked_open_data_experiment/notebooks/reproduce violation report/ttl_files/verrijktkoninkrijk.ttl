:verrijktkoninkrijk a dcat:Dataset ;
    dcat:keyword "Amsterdam", "Dutch", "NIOD", "VU_Amsterdam", "Verrijkt_Koninkrijk", "WW2", "War", "World_War_2", "lod", "lodcloud-diagram-2014-08-30", "publications" ;
