    void:triples "197"^^xsd:integer ;
 .

:geospecies a void:Linkset ;
    void:target :epa-srs ;
    void:triples "21754"^^xsd:integer ;
 .


:contactAgent a prov:CAgent ;
     foaf:Cname "EPA_Office_of_Environmental_Information"^^xsd:string ;
