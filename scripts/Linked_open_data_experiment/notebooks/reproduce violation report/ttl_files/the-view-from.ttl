""" ;
        dcat:accessURL_exampleResource <http://theviewfrom.org/things/en/Banff_Springs_Hotel> ;
        adms:status_exampleResource """
FAIL (500)
""" ;
    ] ;
    void:sparqlEndpoint [
        dct:title_sparqlEndpoint "None" ;
        dcat:endpointDescription_sparqlEndpoint """
