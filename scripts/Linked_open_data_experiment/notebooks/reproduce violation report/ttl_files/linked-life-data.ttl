    void:uriSpace <http://linkedlifedata.com/resource/> ;
    prov:CqualifiedAttribution [
        prov:Cagent :contactAgent ;
        dcat:ChadRole :contact_point ;
    ] .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 

