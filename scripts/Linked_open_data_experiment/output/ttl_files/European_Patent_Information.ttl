""" ;
        dcat:accessURL <http://data.epo.org/linked-data/id/application/EP/01903571.ttl> ;
        adms:status """
FAIL (400)
""" ;
    ] ;
    prov:qualifiedAttribution [
        prov:agent :contactAgent ;
        dcat:hadRole :contact_point ;
    ] .

