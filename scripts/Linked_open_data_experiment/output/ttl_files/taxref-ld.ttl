        dct:mirror "[]" ;
        dct:title "Full_Turtle_dump" ;
        dct:identifier "6f950461-fa04-81aa-afb4-f288ece196cc" ;
        adms:status """
OK
""" ;
    ] ;
    void:exampleResource [
