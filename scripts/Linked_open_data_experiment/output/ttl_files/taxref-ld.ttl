""" ;
        dcat:accessURL <https://github.com/frmichel/taxref-ld/tree/master/dataset/examples> ;
        adms:status """
OK
""" ;
    ] ;
    void:exampleResource [
        dct:title "LD_example" ;
        dcat:mediaType "application/rdf+xml;_charset=UTF-8" ;
