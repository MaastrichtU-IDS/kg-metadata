    ] ;
    dcat:distribution [
        dcat:mediaType "application/rdf+xml;_charset=UTF-8" ;
        dct:description """
RAMEAU subject headings are used at the BnF, in Intermarc, so as to identify the subjects of a resource. 
""" ;
        adms:status """
OK
""" ;
        dct:title "Types_of_RAMEAU_subject_headings" ;
