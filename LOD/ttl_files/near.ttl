    dct:title "Near" ;
    void:exampleResource [
        dct:title "" ;
        dcat:mediaType "text/html;_charset=UTF-8" ;
        dct:description """
Example resource
""" ;
        dcat:accessURL <http://rdfize.com/geo/point/52.467358/-1.7167/> ;
        adms:status """
OK
""" ;
    ] ;
    dcat:distribution [
        dcat:mediaType "text/html;_charset=UTF-8" ;
        dct:description """
API for accessing Near data by WGS84 lat/long coordinates
""" ;
        adms:status """
OK
""" ;
        dct:title "None" ;
        dct:mirror "[]" ;
        dcat:accessURL <http://rdfize.com/geo/point/> ;
    ] ;
    dcat:distribution [
        dcat:mediaType "application/octet-stream" ;
