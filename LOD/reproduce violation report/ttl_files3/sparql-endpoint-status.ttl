
@prefix : <http://example.org/> . 
@prefix dct: <http://purl.org/dc/terms/> . 
@prefix void: <http://rdfs.org/ns/void#> . 
@prefix dcat: <http://www.w3.org/ns/dcat#> . 
@prefix foaf: <http://xmlns.com/foaf/0.1/> . 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix adms:<http://www.w3.org/TR/vocab-adms/>.
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .  # vCard for contact details

:sparql-endpoint-status a dcat:Dataset ;
    dct:license <http://www.opendefinition.org/licenses/odc-odbl> ;
    dct:title_main "SPARQL_Endpoint_Status" ;
    void:sparqlEndpoint [
        dct:title_sparqlEndpoint "SPARQL_endpoint" ;
        dcat:endpointDescription_sparqlEndpoint """

""" ;
        dcat:accessURL_sparqlEndpoint <http://labs.mondeca.com/endpoint/ends> ;
        adms:status_sparqlEndpoint """
FAIL (404)
""" ;
    ] ;
    dcat:distribution [
        dcat:mediaType_distribution "text/html" ;
        dct:description_distribution """

""" ;
        adms:status_distribution """
FAIL (404)
""" ;
        dct:title_distribution "Ontology_(RDF/XML)" ;
        dct:mirror_distribution "[]" ;
        dcat:accessURL_distribution <http://labs.mondeca.com/vocab/endpointStatus> ;
    ] ;
    dct:description_main """
This dataset is generated by SPARQL endpoints status tool: http://labs.mondeca.com/sparqlEndpointsStatus/index.html

It contains availability information (availability, time response, accessibility information, etc.) about all public SPARQL endpoints described in CKAN. HTTP test along with SPARQL protocol compliance are tested. Since availability report (and data) are generated every hour, SPARQL endpoints status dataset is continually increasing. 

"""@en ;

    foaf:page <http://labs.mondeca.com/sparqlEndpointsStatus/endpoint/endpoint.html> ;
    prov:qualifiedAttribution [
        prov:Cagent :contactAgent ;
        dcat:hadRole :contact_point ;
    ] .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 
:ckan a void:Linkset ;
    void:target :sparql-endpoint-status ;
    void:triples "243"^^xsd:integer ;
 .


:contactAgent a prov:Agent ;
     foaf:Cname "Pierre-Yves_Vandenbussche"^^xsd:string ;
     foaf:Cmbox <mailto:pierre-yves.vandenbussche@mondeca.com> ; 
 .

