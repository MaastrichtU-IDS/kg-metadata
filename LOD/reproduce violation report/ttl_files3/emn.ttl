:emn a dcat:Dataset ;
    dcat:keyword "lexicon", "linguistic" ;
    dct:license <http://www.opendefinition.org/licenses/cc-by> ;
    dct:title_main "EMN" ;
    dcat:distribution [
        dcat:mediaType_distribution "application/x-gzip" ;
        dct:description_distribution """
Complete dump of lexicon
""" ;
        adms:status_distribution """
OK
""" ;
        dct:title_distribution "Dump" ;
        dct:mirror_distribution "[]" ;
        dcat:accessURL_distribution <http://data.lider-project.eu/emn/emn.nt.gz> ;
    ] ;
    dct:description_main """
The Terminology of the European Migration Network in RDF
"""@en ;

    void:triples "117115"^^xsd:integer ;
    dcat:theme "linguistics" ;
    prov:qualifiedAttribution [
        prov:Cagent :contactAgent ;
        dcat:hadRole :contact_point ;
    ] .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 
