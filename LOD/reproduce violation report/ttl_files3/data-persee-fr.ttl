    dcat:distribution [
        dcat:mediaType_distribution "application/x-gzip" ;
        dct:description_distribution """
Dump of the alignments between Persons from Persée's collections to several external datasets : [Idref](https://datahub.ckan.io/dataset/idreffr) (about 55,000 links), primarily, but also data.bnf.fr, VIAF, ISNI, dbPedia, etc.
""" ;
        adms:status_distribution """
OK
""" ;
        dct:title_distribution "Linked_Persee_Persons_(tgz_dump)" ;
        dct:mirror_distribution "[]" ;
        dcat:accessURL_distribution <http://data.persee.fr/dumps/PERSEE_person_align_rdf.tar.gz> ;
    ] ;
    dcat:distribution [
        dcat:mediaType_distribution "text/xml" ;
        dct:description_distribution """
Mainly a refined typology for documents defined with sub-classes of elements from the bibo ontology.
""" ;
        adms:status_distribution """
OK
""" ;
        dct:title_distribution "Persées_OWL_schema" ;
        dct:mirror_distribution "[]" ;
        dcat:accessURL_distribution <http://data.persee.fr/ontology/persee-ontology.owl> ;
    ] ;
    dcat:distribution [
        dcat:mediaType_distribution "text/html;_charset=UTF-8" ;
