    dct:license <http://www.opendefinition.org/licenses/cc-by-sa> ;
    dct:title_main "Persée_in_RDF" ;
    void:exampleResource [
        dct:title_exampleResource "Example_:_record_of_a_person_(rdf/xml)" ;
        dcat:mediaType_exampleResource "application/rdf+xml;_charset=UTF-8" ;
        dct:description_exampleResource """
For more information about the content and organisation of our records, have a look at [our data formats](http://data.persee.fr/explorer/schemas-de-donnees/)
""" ;
